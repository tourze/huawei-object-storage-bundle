{
  "name": "tourze/huawei-object-storage-bundle",
  "description": "华为OBS云存储服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-SPL": "*",
    "ext-date": "*",
    "ext-hash": "*",
    "ext-simplexml": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "league/flysystem": "^3.10",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/http-client-contracts": "^3.6",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/file-storage-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\HuaweiObjectStorageBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\HuaweiObjectStorageBundle\\Tests\\": "tests"
    }
  }
